<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <Description>A bot for the Path Of Exile Xbox Discord server. Integrates with PoE Wiki, Price API, Moderation, RSS and more!</Description>
    <Copyright>2018 Kyle Undefined</Copyright>
    <AssemblyTitle>PoE Bot</AssemblyTitle>
    <NeutralLanguage>en-US</NeutralLanguage>
    <TargetFramework>netcoreapp1.0</TargetFramework>
    <AssemblyName>PoE.Bot</AssemblyName>
    <OutputType>Exe</OutputType>
    <PackageId>PoE.Bot</PackageId>
    <RuntimeIdentifiers>win10-x64</RuntimeIdentifiers>
    <PackageTargetFallback>$(PackageTargetFallback);dnxcore50</PackageTargetFallback>
    <GenerateAssemblyConfigurationAttribute>false</GenerateAssemblyConfigurationAttribute>
    <GenerateAssemblyCompanyAttribute>false</GenerateAssemblyCompanyAttribute>
    <GenerateAssemblyProductAttribute>false</GenerateAssemblyProductAttribute>
  </PropertyGroup>

  <ItemGroup Condition=" '$(TargetFramework)' == 'netcoreapp1.0' ">
    <ProjectReference Include="..\PoE.Bot.AssemblyResolver\PoE.Bot.AssemblyResolver.csproj" />
    <PackageReference Include="System.Runtime.Loader" Version="4.3.0" />
    <PackageReference Include="MarkovCore" Version="1.2.1" />
  </ItemGroup>

  <ItemGroup>
    <PackageReference Include="Discord.Addons.EmbedTool" Version="1.0.0" />
    <PackageReference Include="Discord.Addons.Interactive" Version="1.0.1" />
    <PackageReference Include="Discord.Net" Version="2.0.0-beta-00880" />
    <PackageReference Include="Discord.Net.Commands" Version="2.0.0-beta-00880" />
    <PackageReference Include="Discord.Net.Core" Version="2.0.0-beta-00880" />
    <PackageReference Include="Discord.Net.Rest" Version="2.0.0-beta-00880" />
    <PackageReference Include="Discord.Net.Rpc" Version="2.0.0-beta-00860" />
    <PackageReference Include="Discord.Net.Webhook" Version="2.0.0-beta-00880" />
    <PackageReference Include="Discord.Net.WebSocket" Version="2.0.0-beta-00880" />
    <PackageReference Include="HtmlAgilityPack" Version="1.6.13" />
    <PackageReference Include="Microsoft.Extensions.PlatformAbstractions" Version="1.1.0" />
    <PackageReference Include="Newtonsoft.Json" Version="11.0.1-beta3" />
    <PackageReference Include="System.Runtime.Serialization.Primitives" Version="4.3.0" />
  </ItemGroup>

</Project>
